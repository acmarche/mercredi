{{ form_start(form, {'attr':{'id':'zeze'}}) }}
{{ form_errors(form) }}
<div class="row">

    <input type="hidden" name="g-recaptcha-response" id="g-recaptcha-response">

    <div class="col-6">
        {{ form_row(form.nom) }}
    </div>
    <div class="col-6">
        {{ form_row(form.prenom) }}
    </div>
    <div class="col-6">
        {{ form_row(form.email) }}
    </div>
    <div class="col-6">
        {{ form_row(form.plainPassword) }}
    </div>
    <div class="col-6">
        {{ form_row(form.telephone) }}
    </div>
    <div class="col-6">
        {{ form_row(form.agreeTerms) }}
    </div>
</div>
<button class="btn btn-primary mr-1 mb-2" type="button"
        id="recaptcha-btn"
        data-sitekey="{{ recaptcha_site_key }}"
        data-callback='onSubmit'
        data-action='submit'>
    <i class="far fa-save" aria-hidden="true"></i>
    M'enregistrer
</button>

{{ form_end(form) }}
