<h4 class="text-success">Les groupes</h4>
<table class="table table-bordered">
    <thead>
    <tr>
        <th>Nom</th>
        <th>Maximum inscrits par groupe</th>
    </tr>
    </thead>
    <tbody>
    {% for plaineGroupe in plaine.plaineGroupes %}
        <tr>
            <td>{{ plaineGroupe.groupeScolaire.nom }}</td>
            <td>{{ datas[plaineGroupe.groupeScolaire.id]['enfants']|length}} /{{ groupe.inscriptionMaximum }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
