<h4 class="text-success">Plaines</h4>
<hr/>
<table class="table table-bordered">
    <thead>
    <tr>
        <th>Nom</th>
    </tr>
    </thead>
    <tbody>
    {% for plaine in plaines %}
        <tr>
            <td>
                <a href="{{ path('mercredi_admin_plaine_presence_show', {'enfant': enfant.id ,'plaine': plaine.id }) }}"
                   class="card-link">
                    {{ plaine }}
                </a>
            </td>
        </tr>
    {% else %}
        <tr>
            <td>Aucune</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
