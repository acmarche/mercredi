<div class="card-body">
    <ul class="nav nav-tabs" id="tabPresences" role="tablist">
        {% for year in prensencesGrouped|keys %}
            <li class="nav-item">
                <a class="nav-link" id="tab-{{ year }}" data-bs-toggle="tab" href="#year-{{ year }}" role="tab"
                   aria-controls="home" aria-selected="true">{{ year }}</a>
            </li>
        {% endfor %}
    </ul>
    <div class="tab-content" id="presencesContent">
        {% for year, monthData in prensencesGrouped %}
            <div class="tab-pane fade show active" id="year-{{ year }}" role="tabpanel"
                 aria-labelledby="tab-{{ year }}">
                {% for numMonth, presences in monthData %}
                    <h4 class="text-warning">{{ numMonth | mercredi_month_fr }}</h4>
                        {% include '@AcMarcheMercredi/admin/presence/_list.html.twig' %}
                {% endfor %}
            </div>
        {% endfor %}
    </div>

</div>
